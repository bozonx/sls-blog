{
  "name": "p-libereco-blog",
  "version": "1.3.1",
  "description": "Blog of The system of personal freedom",
  "type": "module",
  "author": "(Ivan Kozyrin <ipkozyrin@gmail.com>)",
  "license": "MIT",
  "scripts": {
    "dev": "vitepress dev src",
    "pagefind": "pagefind --verbose --site ./docs --glob '**/{post,page}*/*.html'",
    "build": "vitepress build src && pnpm pagefind",
    "preview": "vitepress preview src",
    "cf:dev": "pnpm build && wrangler dev",
    "cf:deploy": "pnpm build && wrangler deploy",
    "cf:login": "wrangler login"
  },
  "dependencies": {
    "pagefind": "^1.4.0",
    "vitepress": "2.0.0-alpha.9",
    "vitepress-sls-blog-tmpl": "^0.15.0"
  },
  "devDependencies": {
    "wrangler": "^3.72.0"
  }
}
